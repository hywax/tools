  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": "^9.12.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "eslint": "^9.15.0"
  },
  "devDependencies": {
    "@hywax/tools-tsconfig": "workspace:*",
    "unbuild": "^2.0.0"
  }
}
